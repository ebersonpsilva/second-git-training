log4j.rootLogger=INFO,R
log4j.appender.R=org.apache.log4j.rolling.RollingFileAppender
log4j.appender.R.rollingPolicy=org.apache.log4j.rolling.TimeBasedRollingPolicy
log4j.appender.R.rollingPolicy.FileNamePattern=${log.archivo}.%d{yyyyMMdd-HH}.log.zip
log4j.appender.R.layout=org.apache.log4j.PatternLayout
log4j.appender.R.rollingPolicy.ActiveFileName=${log.archivo}
log4j.appender.R.layout.ConversionPattern=%d{yyyy-MM-dd HH:mm:ss.SSS} [%t] [%-5p] - %m%n
